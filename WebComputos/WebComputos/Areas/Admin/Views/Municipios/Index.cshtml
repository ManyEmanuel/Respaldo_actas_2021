    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="container">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Lista de municipios</h2>
        </div>

    </div>
    <br />
    <br />
    <table id="tblMunicipio" class="table table-striped table-bordered " style="width:100%">
