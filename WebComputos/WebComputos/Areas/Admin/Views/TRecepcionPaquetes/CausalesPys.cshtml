
@{
    ViewData["Title"] = "Causales";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <br />
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h2 class="text-primary">Lista de causales en la elección de Presidencias Municipales y Sindicaturas</h2>
            </div>

        </div>
        <br />
        <br />
        <input type="hidden" id="eleccion" value="2" />
        <table id="tblcau" class="table table-striped table-bordered " style="width:100%">
            <thead>
                <tr>
                    <th>Sección</th>
                    <th>Casilla</th>
                    <th>Causal 1</th>
                    <th>Causal 2</th>
                    <th>Causal 3</th>
                    <th>Causal 4</th>
                    <th>Causal 5</th>
                    <th>Causal 6</th>
                    <th>Causal 7</th>
                    <th>Causal 8</th>
                    <th>Causal 9</th>
                    <th>Causal 10</th>
                    <th>Causal 11</th>
                    <th>Número de Causales</th>

                </tr>
            </thead>
        </table>

        <br />
        <br />

        <div class="row">
            <div class="col-md-6">
                <h2 class="text-primary">
                    <p class="text-center">Referencia de causales</p>
                </h2>
                <table class="table table-striped table-bordered " style="width:100%">
                    <thead>
                        <tr>
                            <th>Número</th>
                            <th>Descripción</th>
                        </tr>
                    </thead>
                    <tr>
                        <td>1</td>
                        <td>El Paquete Electoral fue entregado sin el AECC</td>
                    </tr>
                    <tr>
                        <td>2</td>
                        <td>Votos Nulos mayor a la diferencia entre el 1er y 2do lugar</td>
                    </tr>
                    <tr>
                        <td>3</td>
                        <td>Total de votos del sistema mayor a boletas entregadas</td>
                    </tr>
                    <tr>
                        <td>4</td>
                        <td>Total de votos del sistema diferente al total de votos del AECC</td>
                    </tr>
                    <tr>
                        <td>5</td>
                        <td>Votos para un solo Partido Político, Coalición o Candidato Independiente</td>
                    </tr>
                    <tr>
                        <td>6</td>
                        <td>Acta ilegible</td>
                    </tr>
                    <tr>
                        <td>7</td>
                        <td>Acta con alteraciones</td>
                    </tr>
                    <tr>
                        <td>8</td>
                        <td>Paquete con muestras de alteraciones</td>
                    </tr>
                    <tr>
                        <td>9</td>
                        <td>Ciudadanos que votaron diferente al total de votos del sistema</td>
                    </tr>
                    <tr>
                        <td>10</td>
                        <td>Boletas sobrantes más ciudadanos que votaron mayor a boletas entregadas</td>
                    </tr>
                    <tr>
                        <td>11</td>
                        <td>Boletas sobrantes más total de votos del sistema mayor a boletas entregadas</td>
                    </tr>
                </table>
            </div>
            <div class="col-md-6">
                <h2 class="text-primary"> <p class="text-center">Casillas Objeto de Recuento</p></h2>
                <br />
                <div class="row">
                    <div class="col-md-6">
                        <h4 class="text-primary"> <p class="text-center">Cantidad </p></h4>
                        <h5 class="text-primary"><p class="text-center"> @ViewBag.Causal </p></h5>
                    </div>
                    <div class="col-md-6">
                        <h4 class="text-primary"> <p class="text-center">Porcentaje </p></h4>
                        <h5 class="text-primary"> <p class="text-center"> @ViewBag.Porcentaje % </p></h5>
                    </div>
                </div>
            </div>
        </div>
        <br />
        <br />
        <div class="row">
            <div class="col-md-8">
                <div class="row">
                    <div class="col-md-8">
                        <h6><p> Avance de captura de actas  completas (Resultados e información complementaria)</p></h6> <br />
                        <h6><p> Avance de captura de información complementaria de actas </p></h6>
                    </div>
                    <div class="col-md-2">
                        <h6 class="text-primary"><p class="text-center"> @ViewBag.Capturados de @ViewBag.Secciones</p></h6> <br /> <br />
                        <h6 class="text-primary"><p class="text-center"> @ViewBag.Capturados de @ViewBag.Secciones</p>
                    </div>
                    <div class="col-md-2">
                        <h6 class="text-primary"><p class="text-center"> @ViewBag.CapturaPorcent %</p></h6> <br /> <br />
                        <h6 class="text-primary"><p class="text-center"> @ViewBag.CapturaPorcent %</p> </h6>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="row">
                    <div class="col-md-10">
                        <h6><p> Proyectadas para recuento</p></h6> <br /><br />
                        <h6><p> Proyectadas para cotejo </p></h6>

                    </div>
                    <div class="col-md-2">
                        <h6 class="text-primary"><p class="text-center"> @ViewBag.Causal </p></h6> <br /> <br />
                        <h6 class="text-primary"><p class="text-center"> @ViewBag.Cotejo </p> </h6>

                    </div>
                </div>
            </div>
        </div>
    </div>
@section Scripts
        {
    <script src="~/js/CausalPys.js"></script>
}